    "import math\n",
    "\n",
    "def sigmoid(x):\n",
    "    return 1/(1-math.exp(-x))\n",
    "\n",
    "def tanh(x):\n",
    "    return (math.exp(x)-math.exp(-x))/(math.exp(x)+math.exp(-x))"
   ]
  },
